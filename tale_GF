f=open('Tale_gold_fish.txt','w')
f.close()
f1=open('1.txt','r').readline().strip('#')
f=open('Tale_gold_fish.txt','w')
f.write(f1)
f.close()

f2=open('2.txt','r').readline().strip('*')
f=open('Tale_gold_fish.txt','a')
f.write(f2)
f.close()

f4=open('4.txt','r').readline().strip('*')
f=open('Tale_gold_fish.txt','a')
f.write(f4)
f.close()

f5=open('5.txt','r').readline().strip('-')
f=open('Tale_gold_fish.txt','a')
f.write(f5)
f.close()

f6=open('6.txt','r').readline().strip('=')
f=open('Tale_gold_fish.txt','a')
f.write(f6)
f.close()
